datasource db {
  provider = "sqlite" 
  url      = "file:../../db/kaerudb.db"
}

generator client {
  provider = "prisma-client-js"
}

model Record {
  id          Int      @id @default(autoincrement())
  lineId      Int
  statusCd    String
  message     String
  deletedAt   String?
  createdAt   DateTime @default(now())
}